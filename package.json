{
  "name": "talepifyapp",
  "version": "1.0.0",
  "private": true,
  "description": "TalepifyApp - Real Estate Portfolio Management App",
  "author": "TalepifyApp Team",
  "license": "MIT",
  "homepage": "https://github.com/talepifyapp/talepifyapp",
  "repository": {
    "type": "git",
    "url": "https://github.com/talepifyapp/talepifyapp.git"
  },
  "keywords": [
    "react-native",
    "real-estate",
    "portfolio",
    "management",
    "mobile-app"
  ],
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "lint:fix": "eslint . --ext .js,.jsx,.ts,.tsx --fix",
    "start": "react-native start",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "clean": "react-native clean",
    "clean:android": "cd android && ./gradlew clean && cd ..",
    "server": "node public-profile-server.js",
    "media:server": "node server-media.js",
    "clean:ios": "cd ios && xcodebuild clean && cd ..",
    "build:android": "cd android && gradlew.bat assembleRelease && cd ..",
    "news": "node scripts/news-fetcher.js",
    "build:ios": "cd ios && xcodebuild -workspace TalepifyApp.xcworkspace -scheme TalepifyApp -configuration Release -destination generic/platform=iOS build && cd ..",
    "format": "prettier --write \"src/**/*.{js,jsx,ts,tsx,json}\"",
    "format:check": "prettier --check \"src/**/*.{js,jsx,ts,tsx,json}\""
  },
